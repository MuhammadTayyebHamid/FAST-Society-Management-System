  <section class="main">
    <div class="main-top">
      <p>Society Management System</p>
    </div>
    <div class="main-body">
      
      <div class="assignTask-box">
        <h1>Assign Task</h1>
        <form>
          <label>Roll No</label>
          <input type="text" placeholder="Enter Roll No i.e. 24I-0823" />
          <label>Deadline</label>
          <input type="date" />
          <label>Description</label>
          <textarea id="descriptionText" rows="10" cols="50" placeholder="Enter Description ..."></textarea>
          <input type="button" value="Assign" />
        </form>
      </div>
      
      <div class="meetingSchedule-box">
              <h1>Meeting Schedule</h1>
              <form>
                <label>Subject</label>
                <input type="text" placeholder="Enter Subject" />
                <label>Date</label>
                <input type="date" />
                <label>Time</label>
                <input type="time" />
                <label>Venue</label>
                <input id="venueText"type="text" placeholder="Enter Venue" />
                <input id="scheduleBtn" type="button" value="Schedule" />
              </form>
            </div>

                </div>


  </section>
  </div>

